{
  "plugins": [
    "@babel/syntax-dynamic-import", 
    "@babel/plugin-syntax-jsx",
    "@babel/plugin-transform-modules-commonjs",
    "@babel/plugin-transform-runtime"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ], 
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ]
}
